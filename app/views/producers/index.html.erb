<h1>Listing producers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Fb</th>
    <th>Gplus</th>
    <th>Linked</th>
    <th>Twit</th>
    <th>Birth date</th>
    <th>Nationality</th>
    <th>Passport</th>
    <th>Ss</th>
    <th>Address</th>
    <th>Photo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @producers.each do |producer| %>
  <tr>
    <td><%= producer.name %></td>
    <td><%= producer.email %></td>
    <td><%= producer.phone %></td>
    <td><%= producer.fb %></td>
    <td><%= producer.gplus %></td>
    <td><%= producer.linked %></td>
    <td><%= producer.twit %></td>
    <td><%= producer.birth_date %></td>
    <td><%= producer.nationality %></td>
    <td><%= producer.passport %></td>
    <td><%= producer.ss %></td>
    <td><%= producer.address %></td>
    <td><%= producer.photo %></td>
    <td><%= link_to 'Show', producer %></td>
    <td><%= link_to 'Edit', edit_producer_path(producer) %></td>
    <td><%= link_to 'Destroy', producer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Producer', new_producer_path %>
