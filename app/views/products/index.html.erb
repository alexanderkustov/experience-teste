<div class="row">
        <div class="span3" id="wrapper">
        
          <img src="img/ok.jpg" class="img-polaroid hover">
              <p class="texth">PRICE WILL APPEAR HERE</p>
        
         <div class="btn-group">
                <button class="btn btn-large">Buy</button>
                <button class="btn disabled btn-large">$55</button>
                <button class="btn btn-large">View Details</button>
          </div>
            <% @products.each do |product| %>
              <tr>
                <td><%= product.user_id %></td>
                <td><%= product.dt %></td>
                <td><%= product.desc %></td>
                <td><%= product.benefits %></td>
                <td><%= product.estimated_rsp %></td>
                <td><%= product.l_price %></td>
                <td><%= product.benefits_clients %></td>
                <td><%= product.photos %></td>
                <td><%= product.videos %></td>
                <td><%= product.sr2l %></td>
                <td><%= product.refp_number %></td>
                <td><%= product.fixed_costs %></td>
                <td><%= product.gross_margin %></td>
                <td><%= product.bp_link %></td>
                <td><%= product.pp_link %></td>
                <td><%= link_to 'Show', product %></td>
                <td><%= link_to 'Edit', edit_product_path(product) %></td>
                <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
        </div>
</div>